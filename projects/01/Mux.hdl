/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
这个等价于用sel = 1 的假设情况 去覆盖 a， 反之 覆盖 b
Or(Xor(a,sel),And(b,sel))
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Nand(a=a, b=, out=);
}